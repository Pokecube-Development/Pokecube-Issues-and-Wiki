{
  "rules":[
    {
      "preset":"is_ultraspace_river_water",
      "and_preset":"is_ultraspace,in_water",
      "matchers":{
        "biome":{
          "biome_tags":"#pokecube_world:is_ultraspace/is_river"
        }
      }
    },
    {
      "preset":"is_ultraspace_beach_water",
      "and_preset":"is_ultraspace,in_water",
      "matchers":{
        "biome":{
          "biome_tags":"#pokecube_world:is_ultraspace/is_beach"
        }
      }
    },
    {
      "preset":"is_ultraspace_ocean_water",
      "and_preset":"is_ultraspace,in_water",
      "matchers":{
        "biome":{
          "biome_tags":"#pokecube_world:is_ultraspace/is_ocean"
        }
      }
    },
    {
      "preset":"is_ultraspace_cave_water",
      "and_preset":"is_ultraspace,in_water",
      "matchers":{
        "biome":{
          "sub_biomes":"cavewater"
        }
      }
    },
    {
      "preset":"is_ultraspace_cave_lake_shore",
      "and_preset":"is_ultraspace,in_air",
      "matchers":{
        "biome":{
          "sub_biomes":"cavewater"
        }
      }
    },
    {
      "preset":"any_ultraspace_water",
      "or_preset":"is_ultraspace_ocean_water,is_ultraspace_beach_water,is_ultraspace_lake_water,is_ultraspace_river_water,is_ultraspace_cave_water"
    }
  ]
}