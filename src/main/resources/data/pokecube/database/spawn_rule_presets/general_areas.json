{
  "rules":[
    {
      "preset":"wet_areas",
      "matchers":{
        "biome":[
          {
            "biome_tags":"#forge:is_wet"
          },
          {
            "negate":true,
            "biome_tags":"#forge:is_cold,#forge:is_dry"
          }
        ]
      }
    },
    {
      "preset":"dry_areas",
      "matchers":{
        "biome":[
          {
            "biome_tags":"#forge:is_dry"
          },
          {
            "negate":true,
            "biome_tags":"#forge:is_cold,#forge:is_wet"
          }
        ]
      }
    },
    {
      "preset":"hot_areas",
      "matchers":{
        "biome":[
          {
            "biome_tags":"#forge:is_hot"
          },
          {
            "negate":true,
            "biome_tags":"#forge:is_cold,#forge:is_wet"
          }
        ]
      }
    },
    {
      "preset":"cold_areas",
      "matchers":{
        "biome":{
          "biome_tags":"#forge:is_cold"
        }
      }
    },
    {
      "preset":"sandy_areas",
      "matchers":{
        "biome":{
          "biome_tags":"#forge:is_sandy"
        }
      }
    },
    {
      "preset":"snowy_areas",
      "matchers":{
        "biome":{
          "biome_tags":"#forge:is_snowy"
        }
      }
    },
    {
      "preset":"spooky_areas",
      "matchers":{
        "biome":{
          "biome_tags":"#forge:is_spooky"
        }
      }
    },
    {
      "preset":"underground",
      "matchers":{
        "biome":{
          "biome_tags":"#forge:is_underground"
        }
      }
    },
    {
      "preset":"deep_down",
      "matchers":{
        "location":{
          "f":"-y"
        }
      }
    },
    {
      "preset":"sky_high",
      "matchers":{
        "location":{
          "f":"y - 150"
        }
      }
    },
    {
      "preset":"on_surface",
      "matchers":{
        "surface":{
          "type":"surface"
        }
      }
    },
    {
      "preset":"below_surface",
      "matchers":{
        "surface":{
          "type":"underground"
        }
      }
    },
    {
      "preset":"ignore_surface",
      "matchers":{
        "surface":{
          "type":"any"
        }
      }
    }
  ]
}