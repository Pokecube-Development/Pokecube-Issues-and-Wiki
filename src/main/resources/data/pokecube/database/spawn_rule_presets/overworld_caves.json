{
  "rules":[
    {
      "preset":"desert_caves",
      "and_preset":"caves",
      "matchers":{
        "biome":{
          "biome_tags":"#forge:is_sandy"
        }
      }
    },
    {
      "preset":"forest_caves",
      "and_preset":"caves",
      "matchers":{
        "biome":{
          "biome_tags":"#minecraft:is_forest"
        }
      }
    },
    {
      "preset":"mountain_caves",
      "or_preset":"peaks_caves",
      "and_preset":"caves",
      "matchers":{
        "biome":{
          "biome_tags":"#minecraft:is_mountain"
        }
      }
    },
    {
      "preset":"peaks_caves",
      "and_preset":"caves",
      "matchers":{
        "biome":{
          "biome_tags":"#forge:is_peak"
        }
      }
    },
    {
      "preset":"jungle_caves",
      "and_preset":"caves",
      "matchers":{
        "biome":{
          "biome_tags":"#minecraft:is_jungle"
        }
      }
    },
    {
      "preset":"plains_caves",
      "and_preset":"caves",
      "matchers":{
        "biome":{
          "biome_tags":"#forge:is_plains"
        }
      }
    },
    {
      "preset":"caves",
      "or_preset":"underground,cave_subbiome",
      "and_preset":"overworld"
    }
  ]
}