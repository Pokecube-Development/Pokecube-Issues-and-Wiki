{
  "rules":[
    {
      "preset":"river_water",
      "and_preset":"overworld,in_water",
      "matchers":{
        "biome":{
          "biome_tags":"#minecraft:is_river"
        }
      }
    },
    {
      "preset":"lake_water",
      "and_preset":"overworld,in_water",
      "matchers":{
        "biome":{
          "sub_biomes":"lake"
        }
      }
    },
    {
      "preset":"beach_water",
      "and_preset":"overworld,in_water",
      "matchers":{
        "biome":{
          "biome_tags":"#pokecube_world:is_beach"
        }
      }
    },
    {
      "preset":"ocean_water",
      "and_preset":"overworld,in_water",
      "matchers":{
        "biome":{
          "biome_tags":"#minecraft:is_ocean"
        }
      }
    },
    {
      "preset":"cave_water",
      "and_preset":"overworld,in_water",
      "matchers":{
        "biome":{
          "sub_biomes":"cavewater"
        }
      }
    },
    {
      "preset":"swamp_water",
      "and_preset":"overworld,in_water",
      "matchers":{
        "biome":{
          "biome_tags":"#forge:is_swamp"
        }
      }
    },
    {
      "preset":"cave_lake_shore",
      "and_preset":"overworld",
      "matchers":{
        "biome":{
          "sub_biomes":"cavewater"
        }
      }
    },
    {
      "preset":"any_water",
      "or_preset":"ocean_water,beach_water,lake_water,river_water,cave_water"
    }
  ]
}