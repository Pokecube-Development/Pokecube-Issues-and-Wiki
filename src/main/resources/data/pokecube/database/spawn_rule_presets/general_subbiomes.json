{
  "rules":[
    {
      "preset":"all",
      "matchers":{
        "biome":{
          "sub_biomes":"all"
        }
      }
    },
    {
      "preset":"all_nether",
      "and_preset":"all,nether"
    },
    {
      "preset":"all_overworld",
      "and_preset":"all,overworld"
    },
    {
      "preset":"all_overworld_water",
      "and_preset":"all,overworld,in_water"
    },
    {
      "preset":"all_not_caves",
      "and_preset":"all",
      "not_preset":"cave_subbiome"
    },
    {
      "preset":"sky",
      "matchers":{
        "biome":{
          "sub_biomes":"sky"
        }
      }
    },
    {
      "preset":"flowers",
      "matchers":{
        "biome":{
          "sub_biomes":"flower"
        }
      }
    },
    {
      "preset":"hills",
      "matchers":{
        "terrain":{
          "type":"hills"
        }
      }
    },
    {
      "preset":"meteors",
      "matchers":{
        "biome":{
          "sub_biomes":"meteor"
        }
      }
    },
    {
      "preset":"ruins",
      "matchers":{
        "biome":{
          "sub_biomes":"ruin"
        }
      }
    },
    {
      "preset":"industrial",
      "matchers":{
        "biome":{
          "sub_biomes":"industrial"
        }
      }
    },
    {
      "preset":"cave_subbiome",
      "matchers":{
        "biome":{
          "sub_biomes":"cave"
        }
      }
    },
    {
      "preset":"custom_subbiomes",
      "or_preset":"ruins,industrial,meteors,caves"
    }
  ]
}