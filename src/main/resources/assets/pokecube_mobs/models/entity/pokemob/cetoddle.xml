<?xml version="1.0" encoding="utf-8"?>
<ModelAnimator>
 <model>
	<phase name="global" scale="0.33"/>
	<phase name="biWalk" leftLeg="leg1" rightLeg="leg2" leftArm="" rightArm="" duration="10" legAngle="30"/>
    <customTex default="cetoddle">
     <part name="Eye" tex="cetoddle_eye"/>
     <animation diffs="0.5,0" part="Eye" trigger="random:0.005:2"/>
	 <part name="Mouth" tex="cetoddle_mouth"/>
     <animation diffs="0.5,0" part="Mouth" trigger="random:0.005:2"/>
    </customTex>
	<phase type="sitting">
		<part name="body">
	  		<component name="1" posOffset="0,10,0" length="1" startKey="0"/>
		</part>
		<part name="arm1">
	  		<component name="1" rotOffset="0,0,-70" length="8" startKey="0"/>
		</part>
		<part name="arm2">
	  		<component name="1" rotOffset="0,0,70" length="8" startKey="0"/>
		</part>
		<part name="leg1">
	  		<component name="1" rotOffset="-50,30,0" length="8" startKey="0"/>
		</part>
		<part name="foot1">
	  		<component name="1" rotOffset="30,0,0" length="1" startKey="0"/>
		</part>
		<part name="leg2">
	 		<component name="1" rotOffset="-50,-30,0" length="8" startKey="0"/>
		</part>
		<part name="foot2">
	  		<component name="1" rotOffset="30,0,0" length="1" startKey="0"/>
		</part>
		<part name="hair">
	 		<component name="1" rotChange="0,0,6" length="10" startKey="0"/>
	 		<component name="2" rotChange="0,0,-6" length="10" startKey="10"/>
		</part>
		<part name="mouth">
	 		<component name="1" rotOffset="4,0,0" rotChange="4,0,0" length="10" startKey="0"/>
	 		<component name="2" rotChange="-4,0,0" length="10" startKey="10"/>
		</part>
	</phase>
	<phase type="idle">
	    <part name="tail_1">
	 		<component name="1" rotChange="4,0,0" length="10" startKey="0"/>
	 		<component name="2" rotChange="-4,0,0" length="10" startKey="10"/>
		</part>
		<part name="tail_2">
	 		<component name="1" rotChange="4,0,0" length="10" startKey="0"/>
	 		<component name="2" rotChange="-4,0,0" length="10" startKey="10"/>
		</part>
		<part name="arm1">
			<component name="1" rotOffset="0,-10,-40" length="1" startKey="0"/>
			<component name="1" rotChange="0,0,3" length="12" startKey="0"/>
			<component name="2" rotChange="0,0,-3" length="12" startKey="10"/>
		</part>
		<part name="arm2">
			<component name="1" rotOffset="0,10,40" length="1" startKey="0"/>
			<component name="1" rotChange="0,0,-3" length="12" startKey="0"/>
			<component name="2" rotChange="0,0,3" length="12" startKey="10"/>
		</part>
		<part name="leg1">
			<component name="1" rotOffset="0,15,0" length="1" startKey="0"/>
		</part>
		<part name="leg2">
			<component name="1" rotOffset="0,-15,0" length="1" startKey="0"/>
		</part>
	</phase>
	<phase type="walking">
		<part name="body">
			<component name="1" rotChange="0,0,-3" length="10" startKey="0"/>
			<component name="2" rotChange="0,0,6" length="10" startKey="5"/>
			<component name="3" rotChange="0,0,-3" length="10" startKey="10"/>
		</part>
		<part name="tail_1">
	 		<component name="1" rotChange="4,0,0" length="10" startKey="0"/>
	 		<component name="2" rotChange="-4,0,0" length="10" startKey="10"/>
		</part>
		<part name="tail_2">
	 		<component name="1" rotChange="4,0,0" length="10" startKey="0"/>
	 		<component name="2" rotChange="-4,0,0" length="10" startKey="10"/>
		</part>
		<part name="arm1">
			<component name="1" rotOffset="0,0,-10" length="1" startKey="0"/>
			<component name="1" rotChange="0,0,20" length="10" startKey="0"/>
			<component name="2" rotChange="0,0,-40" length="10" startKey="5"/>
			<component name="3" rotChange="0,0,20" length="10" startKey="10"/>
		</part>
		<part name="arm2">
			<component name="1" rotOffset="0,0,10" length="1" startKey="0"/>
			<component name="1" rotChange="0,0,-20" length="10" startKey="0"/>
			<component name="2" rotChange="0,0,40" length="10" startKey="5"/>
			<component name="3" rotChange="0,0,-20" length="10" startKey="10"/>
		</part>
  </phase>
  </model>
</ModelAnimator>