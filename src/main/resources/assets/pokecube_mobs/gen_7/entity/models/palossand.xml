    <component length="10" name="2" rotChange="0,0,-1" startKey="10"/>
   </part>
  </phase>
  <phase type="walking">
   <part name="p">
    <component length="120" name="1" rotChange="0,-360,0" startKey="0"/>
   </part>
   <part name="center">
    <component length="10" name="1" scaleChange="0,-0.08,0" startKey="0"/>
    <component length="10" name="2" scaleChange="0,0.08,0" startKey="10"/>
   </part>
   <part name="arm1">
    <component length="10" name="1" rotChange="30,0,0" startKey="0"/>
