    <component length="10" name="1" rotOffset="0,0,-20" startKey="0"/>
   </part>
   <part name="arm2">
    <component length="10" name="1" rotOffset="0,0,20" startKey="0"/>
   </part>
   <part name="arm2_1">
    <component length="10" name="1" rotOffset="0,0,20" startKey="0"/>
   </part>
  </phase>
  <phase type="idle">
   <part name="p">
