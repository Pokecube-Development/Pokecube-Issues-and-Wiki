{
  "multipart": [
    {
      "when": {  "up": "true", "flowers": "false" },
      "apply": { "model": "pokecube_legends:block/string_of_pearls", "x": 270, "uvlock": true }
    },
    {
      "when": {  "up": "true", "flowers": "true" },
      "apply": [
        { "model": "pokecube_legends:block/string_of_pearls_flowering", "x": 270, "uvlock": true },
        { "model": "pokecube_legends:block/string_of_pearls_flowering2", "x": 270, "uvlock": true }
      ]
    },
    {
      "when": { "west": "true", "flowers": "false" },
      "apply": { "model": "pokecube_legends:block/string_of_pearls", "y": 270, "uvlock": true }
    },
    {
      "when": { "west": "true", "flowers": "true" },
      "apply": [
        { "model": "pokecube_legends:block/string_of_pearls_flowering", "y": 270, "uvlock": true },
        { "model": "pokecube_legends:block/string_of_pearls_flowering2", "y": 270, "uvlock": true }
      ]
    },
    {
      "when": { "east": "true", "flowers": "false" },
      "apply": { "model": "pokecube_legends:block/string_of_pearls", "y": 90, "uvlock": true }
    },
    {
      "when": { "east": "true", "flowers": "true" },
      "apply": [
        { "model": "pokecube_legends:block/string_of_pearls_flowering", "y": 90, "uvlock": true },
        { "model": "pokecube_legends:block/string_of_pearls_flowering2", "y": 90, "uvlock": true }
      ]
    },
    {
      "when": { "south": "true", "flowers": "false" },
      "apply": { "model": "pokecube_legends:block/string_of_pearls", "y": 180, "uvlock": true }
    },
    {
      "when": { "south": "true", "flowers": "true" },
      "apply": [
        { "model": "pokecube_legends:block/string_of_pearls_flowering", "y": 180, "uvlock": true },
        { "model": "pokecube_legends:block/string_of_pearls_flowering2", "y": 180, "uvlock": true }
      ]
    },
    {
      "when": { "north": "true", "flowers": "false" },
      "apply": { "model": "pokecube_legends:block/string_of_pearls" }
    },
    {
      "when": { "north": "true", "flowers": "true" },
      "apply": [
        { "model": "pokecube_legends:block/string_of_pearls_flowering" },
        { "model": "pokecube_legends:block/string_of_pearls_flowering2" }
      ]
    }
  ]
